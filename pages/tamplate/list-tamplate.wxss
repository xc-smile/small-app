/* pages/tamplate/list-tamplate.wxss */
.list_container{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin : 20rpx 0;
  border-bottom: 2rpx solid #eee;
  border-top: 2rpx solid #eee;
}
.avatar_date image{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.title{
  font-size: 38rpx;
  font-weight: 900;
  color: pink;
}
.contentImg{
  width: 100%;
  height: 460rpx;
}
.content{
  font-size: 32rpx;
  text-indent: 32rpx;
}
.star image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.star text{
  margin-right: 20rpx;
}